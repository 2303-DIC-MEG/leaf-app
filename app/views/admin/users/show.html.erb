<br>

<h2><%= @user.name %> Task</h2>
  <table class="table table-striped">
    <tr>
      <th>Title</th>
      <th>Content</th>
      <th>Deadline</th>
      <th>Created_at</th>
      <th>Status</th>
      <th>Priority</th>
    </tr>
    <% @user.tasks.each do |task| %>
      <tr>
        <td><%= task.title %></td>
        <td><%= task.content %></td>
        <td><%= task.expired_at %></td>
        <td><%= task.created_at %></td>
        <td><%= task.status %></td>
        <td><%= task.priority %></td>
      </tr>
    <% end %>
  </table>
<%= link_to 'Back', admin_users_path %>